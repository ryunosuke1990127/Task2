<h1>Books</h1>
<% @lists.each do |list| %>
<p>Titel</p>
<span><%= list.title %></span>
<p>Body</p>
<span><%= list.body %></span>
<% end %>



<h1>New Book</h1>

<!--新規投稿フォームの作成-->
<%= form_with model: @book do |f|%>
<h4>Title</h4>
<%= f.text_field :title %>
<h4>Body</h4>
<%= f.text_area :body %>
<%= f.submit 'create book'%>
<% end %>

